    "start:worker": "node dist/saas/jobs/worker.js",
    "start:worker:prod": "infisical run --token=\"$INFISICAL_TOKEN\" --projectId=\"$PROJECT_ID\" --env=\"$INFISICAL_SECRET_ENV\" --domain=\"$INFISICAL_API_URL\" -- node dist/saas/jobs/worker-with-aliases.js"
